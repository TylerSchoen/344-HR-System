<%- partial ('navbar.ejs') %>

<div class="container">
    <div class="row">    
        <div class="col-sm-6 col-md-offset-1 col-md-10">
            <h3>getUserById(1)</h3>
            <table class="table table-striped">
            <thead>
                <tr>
                    <th>User</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                <% if (error) { %>
                    <%= error %>
                <% } else { %>
                    <% for(var i=0; i < userInfo.length; i++) { %>
                <tr>
                    <th scope="row"><%= userInfo[i].Firstname %> <%= userInfo[i].Lastname %></th>
                    <td><%= userInfo[i].Email %></td>
                </tr>
                    <% } %>
                <% } %>
            </tbody>
            </table>
	</div>
</div>